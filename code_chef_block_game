#include <iostream>
using namespace std;

int main() {
	int t,reverse,remainder;
	cin>>t;
for(int i=0;i<t;i++)
{

	    int n;
	    cin>>n;
	    reverse=0;
	    int original=n;
	    while(n!=0){
	        remainder=n%10;
	        reverse=reverse*10+remainder;
	        n=n/10;
	    }
	
	if(reverse==original){
	    cout<<"wins"<<endl;
	}
	else{
	    cout<<"loses"<<endl;
	}
}
}
